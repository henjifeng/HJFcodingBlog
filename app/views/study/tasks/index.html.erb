<p id="notice"><%= notice %></p>

<h1>Study Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Important</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @study_tasks.each do |study_task| %>
      <tr>
        <td><%= study_task.title %></td>
        <td><%= study_task.important %></td>
        <td><%= link_to 'Show', study_task %></td>
        <td><%= link_to 'Edit', edit_study_task_path(study_task) %></td>
        <td><%= link_to 'Destroy', study_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Study Task', new_study_task_path %>
